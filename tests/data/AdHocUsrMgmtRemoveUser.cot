send: {
<iq type="set" id="aadca" >
  <command xmlns="http://jabber.org/protocol/commands" node="http://jabber.org/protocol/admin#new-command" >
    <x xmlns="jabber:x:data" type="submit" >
      <field type="jid-single" var="rosterOwnerJID" >
	<value>$(to-jid)</value>
      </field>
      <field type="jid-single" var="jidToManipulate" >
	<value>$(from-id)</value>
      </field>
      <field type="text-single" var="groups" >
	<value></value>
      </field>
      <field type="list-single" var="operationType" >
	<value>removeJid</value>
      </field>
      <field type="list-single" var="subscriptionType" >
	<value>both</value>
      </field>
    </x>
  </command>
</iq>
}

expect: {
<iq type="result" id="aadca" >
  <command xmlns="http://jabber.org/protocol/commands" status="completed" node="http://jabber.org/protocol/admin#new-command" >
    <x xmlns="jabber:x:data" type="result" >
      <field type="fixed" var="Note" >
	<value>Operation successful</value>
      </field>
    </x>
  </command>
</iq>
}
