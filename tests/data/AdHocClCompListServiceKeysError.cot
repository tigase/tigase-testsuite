send: {
<iq id="service-keys"
    from="$(from-jid)"
    to="$(to-jid)"
    type="set">
    <command xmlns="http://jabber.org/protocol/commands"
             node="service-keys" />
</iq>
}

expect: {
<iq from="$(to-jid)"
    to="$(from-jid)"
    type="result">
    <command
        xmlns="http://jabber.org/protocol/commands">
		<x xmlns="jabber:x:data">
			<field var="Error" type="fixed">
				<value></value>
			</field>
		</x>
	</command>
</iq>
}


