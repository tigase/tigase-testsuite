send: {
<iq id="s2s-bad-state-conns"
    from="$(from-jid)"
    to="$(to-jid)"
    type="set">
    <command xmlns="http://jabber.org/protocol/commands"
             node="s2s-bad-state-conns" />
</iq>
}

expect: {
<iq id="s2s-bad-state-conns"
    from="$(to-jid)"
    to="$(from-jid)"
    type="result">
    <command
        xmlns="http://jabber.org/protocol/commands"
        node="s2s-bad-state-conns"
        status="completed">
        <x xmlns="jabber:x:data" type="result">
			<field type="text-multi" var="Connections with waiting packets">
				<value></value>
			</field>
		</x>
	</command>
</iq>
}
